  "body": "<!DOCTYPE html>\r\n<html lang=\"en-us\">\r\n  <head>\r\n    <meta charset=\"UTF-8\">\r\n    <script src=\"https://code.jquery.com/jquery-2.1.4.js\"></script>\r\n    <title>Awesome Planets from Digital Makers</title>\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\r\n    <link rel=\"stylesheet\" type=\"text/css\" href=\"stylesheets/normalize.css\" media=\"screen\">\r\n    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,700' rel='stylesheet' type='text/css'>\r\n    <link rel=\"stylesheet\" type=\"text/css\" href=\"stylesheets/stylesheet.css\" media=\"screen\">\r\n    <link rel=\"stylesheet\" type=\"text/css\" href=\"stylesheets/github-light.css\" media=\"screen\">\r\n  </head>\r\n  <body>\r\n    <section class=\"page-header\">\r\n      <h1 class=\"project-name\">Awesome Planets</h1>\r\n      <h2 class=\"project-tagline\">From the boys and girls who code in Digital Makers at the Guild.</h2>\r\n    </section>\r\n\r\n    <section class=\"main-content\">\r\n\r\n<svg height=\"600\" width=\"600\">\r\n  <polygon points=\"0,0 0,600 600,600 600,0\" fill=\"black\" />\r\n\r\n  <!-- sun -->\r\n  <circle id=\"Sun\" cx=\"300\" cy=\"300\" r=\"50\" fill=\"url(#venus)\" />\r\n\r\n  <!-- mercury -->\r\n  <circle id=\"Mercury\" cx=\"300\" cy=\"230\" r=\"5\" fill=\"url(#mercury)\" />\r\n  <circle cx=\"300\" cy=\"300\" r=\"70\" stroke-width=\"1\" stroke=\"white\" fill-opacity=\"0.0\" />\r\n\r\n  <!-- venus -->\r\n  <circle id=\"Venus\" cx=\"300\" cy=\"210\" r=\"10\" fill=\"url(#venus)\" />\r\n  <circle cx=\"300\" cy=\"300\" r=\"90\" stroke-width=\"1\" stroke=\"white\" fill-opacity=\"0.0\" />\r\n\r\n  <!-- earth -->\r\n  <circle id=\"Earth\" cx=\"300\" cy=\"180\" r=\"10\" fill=\"url(#earth)\" />\r\n  <circle cx=\"300\" cy=\"300\" r=\"120\" stroke-width=\"1\" stroke=\"white\" fill-opacity=\"0.0\" />\r\n\r\n  <!-- mars -->\r\n  <circle cx=\"300\" cy=\"150\" r=\"10\" fill=\"url(#mars)\" />\r\n  <circle cx=\"300\" cy=\"300\" r=\"150\" stroke-width=\"1\" stroke=\"white\" fill-opacity=\"0.0\" />\r\n\r\n  <!-- jupiter -->\r\n  <circle id=\"jupiter\" cx=\"300\" cy=\"110\" r=\"20\" fill=\"url(#jupiter)\" />\r\n  <circle cx=\"300\" cy=\"300\" r=\"190\" stroke-width=\"1\" stroke=\"white\" fill-opacity=\"0.0\" />\r\n\r\n  <!-- saturn -->\r\n  <circle id=\"Saturn\" cx=\"300\" cy=\"60\" r=\"15\" fill=\"url(#saturn)\" />\r\n  <circle id=\"SaturnRing\" cx=\"300\" cy=\"60\" r=\"25\" stroke-width=\"1\" stroke=\"white\" fill-opacity=\"0.0\" />\r\n <circle cx=\"300\" cy=\"300\" r=\"240\" stroke-width=\"1\" stroke=\"white\" fill-opacity=\"0.0\" />\r\n\r\n  <linearGradient id=\"earth\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\r\n    <stop offset=\"0%\" stop-color=\"#00ff00\" />\r\n    <stop offset=\"100%\" stop-color=\"#0000ff\" />\r\n  </linearGradient>\r\n\r\n  <linearGradient id=\"mercury\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\r\n    <stop offset=\"0%\" stop-color=\"#b0b0b0\" />\r\n    <stop offset=\"100%\" stop-color=\"#ffffff\" />\r\n  </linearGradient>\r\n\r\n  <linearGradient id=\"venus\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\r\n    <stop offset=\"0%\" stop-color=\"#ff0000\" />\r\n    <stop offset=\"100%\" stop-color=\"#fff000\" />\r\n  </linearGradient>\r\n\r\n  <linearGradient id=\"mars\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\r\n    <stop offset=\"0%\" stop-color=\"#ff0000\" />\r\n    <stop offset=\"100%\" stop-color=\"#fff00\" />\r\n  </linearGradient>\r\n\r\n  <linearGradient id=\"jupiter\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\r\n    <stop offset=\"0%\" stop-color=\"#ff0000\" />\r\n    <stop offset=\"100%\" stop-color=\"#fff00\" />\r\n  </linearGradient>\r\n\r\n  <linearGradient id=\"saturn\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\r\n    <stop offset=\"0%\" stop-color=\"#fdff86\" />\r\n    <stop offset=\"100%\" stop-color=\"#fff000\" />\r\n  </linearGradient>\r\n\r\n  <linearGradient id=\"sun\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\r\n    <stop offset=\"0%\" stop-color=\"#fff00f\" />\r\n    <stop offset=\"100%\" stop-color=\"#fff000\" />\r\n  </linearGradient>\r\n</svg>\r\n\r\n<script id=\"louis-planets\">\r\nvar saturn = 0;\r\nvar earth = 0;\r\nvar venus = 0;\r\nvar mercury = 0;\r\n\r\nfunction movePlanets() {\r\n\r\n  saturn = saturn + 0.005;\r\n  earth = earth + 0.04;\r\n  venus = venus + 0.06;\r\n  mercury = mercury + 0.02;\r\n  \r\n\r\n  console.log(\"moving earth \" + earth);\r\n  var earthX = calculateXPosition(earth, 300, 120);\r\n  var earthY = calculateYPosition(earth, 300, 120);\r\n  console.log(\"earth x=\" + earthX + \" y = \" + earthY);\r\n  \r\n  var saturnX = calculateXPosition(saturn, 300, 240);\r\n  var saturnY = calculateYPosition(saturn, 300, 240);\r\n  console.log(\"saturn x=\" + saturnX + \" y = \" + saturnY);\r\n  \r\n   var SaturnRingX = calculateXPosition(saturn, 300, 240);\r\n  var SaturnRingY = calculateYPosition(saturn, 300, 240);\r\n  console.log(\"saturnring x=\" + SaturnRingX + \" y = \" + SaturnRingY);\r\n  \r\n  var VenusX = calculateXPosition(venus, 300, 90);\r\n  var VenusY = calculateYPosition(venus, 300, 90);\r\n  console.log(\"Venus x=\" + VenusX + \" y = \" + VenusY);\r\n  \r\n  var MercuryX = calculateXPosition(mercury, 300, 70);\r\n  var MercuryY = calculateYPosition(mercury, 300, 70);\r\n  console.log(\"Mercury x=\" + MercuryX + \" y = \" + MercuryY);\r\n\r\n  $(\"#Earth\").attr(\"cx\", earthX);\r\n$(\"#Earth\").attr(\"cy\", earthY);\r\n\r\n $(\"#Saturn\").attr(\"cx\", saturnX);\r\n$(\"#Saturn\").attr(\"cy\", saturnY);\r\n\r\n $(\"#SaturnRing\").attr(\"cx\", SaturnRingX);\r\n$(\"#SaturnRing\").attr(\"cy\", SaturnRingY);\r\n\r\n $(\"#Venus\").attr(\"cx\", VenusX);\r\n$(\"#Venus\").attr(\"cy\", VenusY);\r\n\r\n $(\"#Mercury\").attr(\"cx\", MercuryX);\r\n$(\"#Mercury\").attr(\"cy\", MercuryY);\r\n\r\n}\r\n\r\nfunction calculateXPosition(degrees, sunX, orbitRadius) {\r\n  var velocity = degrees * (Math.PI / 180);\r\n  return sunX + (orbitRadius * Math.cos(velocity));\r\n}\r\n\r\nfunction calculateYPosition(degrees, sunY, orbitRadius) {\r\n  var velocity = degrees * (Math.PI / 180);\r\n  return sunY + (orbitRadius * Math.sin(velocity));\r\n}\r\n\r\nsetInterval(movePlanets, 2.5);\r\n\r\n    </script>\r\n      <footer class=\"site-footer\">\r\n        <span class=\"site-footer-owner\"><a href=\"https://github.com/JohnReeves/LouisPlanets\">Awesome Planets</a> is maintained by <a href=\"https://github.com/JohnReeves\">JohnReeves</a>.</span>\r\n        <span class=\"site-footer-credits\">This page was generated by <a href=\"https://pages.github.com\">GitHub Pages</a> using the <a href=\"https://github.com/jasonlong/cayman-theme\">Cayman theme</a> by <a href=\"https://twitter.com/jasonlong\">Jason Long</a>.</span>\r\n      </footer>\r\n    </section>\r\n    \r\n  </body>\r\n</html>\r\n",
